{
  "name": "satellite",
  "displayName": "Satellite",
  "version": "0.0.2",
  "description": "Eve-Sama's personal browser extension",
  "author": "Eve-Sama",
  "scripts": {
    "start": "plasmo dev",
    "build:firefox": "npx plasmo build --target=firefox-mv2",
    "build:chrome": "npx plasmo build --target=chrome-mv3"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.5.0",
    "@plasmohq/storage": "^1.8.0",
    "antd": "^5.9.3",
    "plasmo": "0.83.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@types/chrome": "0.0.245",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "scripting"
    ]
  }
}
